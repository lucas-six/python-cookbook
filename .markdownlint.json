{
    "default": true,
    "first-line-heading": true,
    "no-duplicate-heading": {
        "siblings_only": true
    },
    "line-length": {
        "line_length": 100,
        "heading_line_length": 60,
        "code_blocks": false,
        "tables": false
    },
    "no-inline-html": {
        "allowed_elements": [
            "section",
            "p",
            "a",
            "code",
            "br",
            "img"
        ]
    }
}
